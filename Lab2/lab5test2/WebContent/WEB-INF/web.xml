<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee">
    <display-name>Тестовый пример</display-name>
    <welcome-file-list>
        <welcom-file>index.jsp</welcom-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CathedraListServlet</servlet-name>
        <servlet-class>CathedraListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CathedraListServlet</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CathedraEditServlet</servlet-name>
        <servlet-class>CathedraEditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CathedraEditServlet</servlet-name>
        <url-pattern>/edit.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CathedraSaveServlet</servlet-name>
        <servlet-class>CathedraSaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CathedraSaveServlet</servlet-name>
        <url-pattern>/CathedraSave.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CathedraDeleteServlet</servlet-name>
        <servlet-class>CathedraDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CathedraDeleteServlet</servlet-name>
        <url-pattern>/CathedraDelete.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeacherListServlet</servlet-name>
        <servlet-class>TeacherListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherListServlet</servlet-name>
        <url-pattern>/TeacherIndex.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeacherEditServlet</servlet-name>
        <servlet-class>TeacherEditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherEditServlet</servlet-name>
        <url-pattern>/TeacherEdit.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeacherSaveServlet</servlet-name>
        <servlet-class>TeacherSaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherSaveServlet</servlet-name>
        <url-pattern>/TeacherSave.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeacherDeleteServlet</servlet-name>
        <servlet-class>TeacherDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherDeleteServlet</servlet-name>
        <url-pattern>/TeacherDelete.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login-form.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserEditServlet</servlet-name>
        <servlet-class>UserEditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserEditServlet</servlet-name>
        <url-pattern>/UserEdit.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserSaveServlet</servlet-name>
        <servlet-class>UserSaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserSaveServlet</servlet-name>
        <url-pattern>/UserSave.html</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserDeleteServlet</servlet-name>
        <servlet-class>UserDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserDeleteServlet</servlet-name>
        <url-pattern>/UserDelete.html</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>SecurityFilter</filter-name>
        <filter-class>SecurityFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>SecurityFilter</filter-name>
        <url-pattern>/edit.html</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SecurityFilter</filter-name>
        <url-pattern>/save.html</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SecurityFilter</filter-name>
        <url-pattern>/delete.html</url-pattern>
    </filter-mapping>

</web-app>